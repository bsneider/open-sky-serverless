{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowS3Get",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "${s3_arn}",
        "${s3_arn}/*"
      ]
    },
    {
      "Sid": "AllowRDSDescribe",
      "Effect": "Allow",
      "Action": [
        "rds:*"
      ],
      "Resource": [
        "${rds_arn}"
      ]
    }
  ]
}